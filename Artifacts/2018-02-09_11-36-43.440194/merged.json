{
 "scenarios": {
  "BlazeDemo_confirmation": {
   "requests": [
    {
     "url": "http://www.blazedemo.com", 
     "method": "GET", 
     "label": "home"
    }, 
    {
     "url": "http://www.blazedemo.com/reserve.php", 
     "body": {
      "toPort": "${toPort}", 
      "fromPort": "${fromPort}"
     }, 
     "method": "POST", 
     "label": "reserve"
    }, 
    {
     "url": "http://www.blazedemo.com/purchase.php", 
     "body": {
      "toPort": "${toPort}", 
      "fromPort": "${fromPort}", 
      "flight": "12", 
      "airline": "Virgin America"
     }, 
     "method": "POST", 
     "label": "purchase"
    }, 
    {
     "body": {
      "creditCardYear": 2018, 
      "city": "pirate cove", 
      "creditCardMonth": 11, 
      "inputName": "captain herp a derp", 
      "zipCode": 95050, 
      "state": "CA", 
      "address": "123 tall ship st.", 
      "creditCardNumber": 1234123412341234, 
      "nameOnCard": "captain herp a derp", 
      "cardType": "visa"
     }, 
     "url": "http://www.blazedemo.com/confirmation.php", 
     "label": "confirmation", 
     "assert": [
      "Thank you for your purchase today!"
     ], 
     "method": "POST", 
     "subject": "body"
    }
   ], 
   "data-sources": [
    {
     "path": "/Users/chefe02/Documents/Demo/blazedemo/blazedemoports.csv", 
     "delimiter": ",", 
     "quoted": false, 
     "variable-names": "fromPort,toPort"
    }
   ]
  }
 }, 
 "execution": [
  {
   "scenario": "BlazeDemo_confirmation", 
   "ramp-up": "5m", 
   "locations": {
    "us-west-1": 1
   }, 
   "concurrency": {
    "local": 1, 
    "cloud": 500
   }, 
   "executor": "jmeter", 
   "hold-for": "10m"
  }
 ], 
 "modules": {
  "blazemeter": {
   "test": "BlazeDemo from Taurus", 
   "project": "BlazeDemo", 
   "token": "********"
  }, 
  "passfail": {
   "criteria": [
    {
     "TooSlow": "avg-rt>300ms, continue as failed"
    }, 
    {
     "Sucked": "failures>=5%, continue as failed"
    }
   ]
  }
 }, 
 "reporting": [
  {
   "module": "final-stats", 
   "dump-xml": "./final_stats.xml"
  }
 ]
}
