{
 "local-bzt-version": "1.10.3", 
 "scenarios": {
  "BlazeDemo_confirmation": {
   "requests": [
    {
     "think-time": null, 
     "body": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://www.blazedemo.com", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "home", 
     "headers": {}, 
     "timeout": null, 
     "method": "GET", 
     "keepalive": null
    }, 
    {
     "body": {
      "toPort": "${toPort}", 
      "fromPort": "${fromPort}"
     }, 
     "think-time": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://www.blazedemo.com/reserve.php", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "reserve", 
     "headers": {}, 
     "timeout": null, 
     "method": "POST", 
     "keepalive": null
    }, 
    {
     "body": {
      "toPort": "${toPort}", 
      "fromPort": "${fromPort}", 
      "flight": "12", 
      "airline": "Virgin America"
     }, 
     "think-time": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://www.blazedemo.com/purchase.php", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "purchase", 
     "headers": {}, 
     "timeout": null, 
     "method": "POST", 
     "keepalive": null
    }, 
    {
     "body": {
      "creditCardYear": 2018, 
      "city": "pirate cove", 
      "creditCardMonth": 11, 
      "inputName": "captain herp a derp", 
      "zipCode": 95050, 
      "state": "CA", 
      "address": "123 tall ship st.", 
      "creditCardNumber": 1234123412341234, 
      "nameOnCard": "captain herp a derp", 
      "cardType": "visa"
     }, 
     "think-time": null, 
     "body-file": null, 
     "content-encoding": null, 
     "url": "http://www.blazedemo.com/confirmation.php", 
     "follow-redirects": null, 
     "upload-files": [], 
     "label": "confirmation", 
     "assert": [
      "Thank you for your purchase today!"
     ], 
     "headers": {}, 
     "timeout": null, 
     "keepalive": null, 
     "method": "POST", 
     "subject": "body"
    }
   ], 
   "data-sources": [
    {
     "path": "blazedemoports.csv", 
     "delimiter": ",", 
     "quoted": false, 
     "variable-names": "fromPort,toPort"
    }
   ], 
   "script": null
  }
 }, 
 "execution": [
  {
   "scenario": "BlazeDemo_confirmation", 
   "ramp-up": "5m", 
   "locations": {
    "us-west-1": 1
   }, 
   "concurrency": 500, 
   "executor": "jmeter", 
   "hold-for": "10m"
  }
 ], 
 "modules": {
  "gatling": {
   "properties": {
    "gatling.data.file.bufferSize": 256
   }
  }, 
  "grinder": {
   "properties": {
    "grinder.useConsole": false
   }
  }, 
  "jmeter": {
   "properties": {
    "jmeter.save.saveservice.autoflush": "true", 
    "jmeter.save.saveservice.connect_time": "true", 
    "summariser.name": "", 
    "jmeterengine.force.system.exit": "true"
   }
  }
 }, 
 "services": [
  {
   "run-at": "local", 
   "local": [
    {
     "metrics": [
      "cpu", 
      "mem", 
      "bytes-sent", 
      "bytes-recv", 
      "disk-read", 
      "disk-write", 
      "disk-space", 
      "engine-loop", 
      "conn-all"
     ]
    }
   ], 
   "module": "monitoring"
  }
 ]
}
