{
 "settings": {
  "check-updates": true, 
  "check-interval": 1, 
  "verbose": false, 
  "aggregator": "consolidator", 
  "default-executor": "jmeter", 
  "proxy": {}, 
  "artifacts-dir": "%Y-%m-%d_%H-%M-%S.%f", 
  "tab-replacement-spaces": 4
 }, 
 "reporting": [
  {
   "dump-csv": null, 
   "module": "final-stats", 
   "summary": true, 
   "test-duration": true, 
   "percentiles": true, 
   "dump-xml": null, 
   "failed-labels": false
  }, 
  {
   "module": "console"
  }, 
  {
   "dump-csv": null, 
   "module": "final-stats", 
   "summary": true, 
   "test-duration": true, 
   "percentiles": true, 
